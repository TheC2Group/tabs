{
  "name": "c2-tabs",
  "version": "2.2.4",
  "description": "Accessible tabs, where aria states are automatically added.",
  "jsnext:main": "tabs.js",
  "main": "cjs/tabs.js",
  "export": "Tabs",
  "scripts": {
    "iife": "rollup -c rollup-iife.config.js",
    "cjs": "rollup -c rollup-cjs.config.js",
    "umd": "rollup -c rollup-umd.config.js",
    "build": "npm run iife && npm run cjs && npm run umd",
    "bundle-example": "browserify test/js/index.js -o test/bundle/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:TheC2Group/tabs.git"
  },
  "keywords": [
    "tabs"
  ],
