  "copyright": "(c) The C2 Group (c2experience.com)",
  "bugs": {
    "url": "https://github.com/TheC2Group/tabs/issues"
  },
  "homepage": "https://github.com/TheC2Group/tabs",
  "dependencies": {
    "c2-event-handler": "^2.5.2",
    "jquery": ">=3.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "babel-preset-es2015-rollup": "^3.0.0",
    "browserify": "^16.2.3",
    "acorn": "^6.0.0",
    "rollup": "^1.12.3",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.0.0"
